# .env.example for React + Vite

# Base API URL of your Laravel backend
VITE_API_BASE_URL=http://127.0.0.1:8000/api

# Public app name
VITE_APP_NAME=MyReactApp

# Feature toggles (examples)
VITE_FEATURE_X_ENABLED=true

# Auth token storage key (example)
VITE_AUTH_TOKEN_KEY=auth_token

# Sentry or other monitoring (example)
VITE_SENTRY_DSN=

# Example: environment
VITE_APP_ENV=development
